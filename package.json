{
  "private": true,
  "name": "project_new_skeleton_typescript",
  "version": "1.0.0",
  "description": "TypeScript project skeleton",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn jest:node",
    "jest:node": "jest -c ./jest.config.node.js",
    "jest:web": "jest -c ./jest.config.web.js",
    "karma": "karma start",
    "karma:window": "yarn webpack && cross-env TEST_ENV=window karma start",
    "tsc": "tsc --build ./tsconfig.json",
    "webpack": "webpack --mode development --config webpack.config.js",
    "webpack:prod": "webpack --mode production --config webpack.config.js",
    "build": "rm -rf ./dist && yarn tsc && yarn webpack:prod",
    "cleanup": "rm -rf ./dist coverage ./node_modules"
  },
  "author": "Jun Kurihara",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.2.0",
    "jasmine-core": "3.10.1",
    "jest": "27.3.1",
    "karma": "6.3.8",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-jasmine": "4.0.1",
    "karma-typescript": "5.5.2",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "typescript": "4.4.4",
    "webpack": "5.64.1",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {}
}
